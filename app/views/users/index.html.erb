<div class="users-new-wrapper">
  <div class="container">
    <div class="row">
      <div class="col-md-offset-4 col-md-4 users-new-container">
        <h1 class="text-center text-gray title">ユーザー一覧</h1>
        <% @user.each do |user| %>
          <p><%= link_to user.name, edit_user_path(user.id) %></p>
        <% end %>
        <%= link_to "新規登録", new_user_path %>
        <%= current_user.admin %>
      </div>
    </div>
  </div>
</div>
